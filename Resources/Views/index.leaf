<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">

  <title>#(page_title) | Reminders</title>
</head>

<body>
    <h1>Reminders</h1>
    <table>
        <thead><td>Title</td><td>Description</td></thead>
        #loop(reminders, "reminder") {
            <tr><td>#(reminder.title)</td><td>#(reminder.description)</td></tr>
        }
    </table>
</body>
</html>
